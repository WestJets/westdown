  Next
  'Convert model name to ASCII
  ProductId = ""
  For I = 0 To Ubound(objMonitor.UserFriendlyName)
    ProductId = ProductId &amp; Chr(objMonitor.UserFriendlyName(I))
  Next
  Output.Write ProductId &amp; " " &amp; SerialNo
Next</script>
